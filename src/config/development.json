{
  "dbConfig": {
    "host": "postgres",
    "port": 5432,
    "database": "tamagotchi",
    "user": "postgres"
  },
  "goodConfig": {
    "ops": {
      "interval": 1000
    },
    "reporters": {
      "console": [
        {
          "module": "@hapi/good-squeeze",
          "name": "Squeeze",
          "args": [
            {
              "log": "*",
              "response": {
                "exclude": ["no-logging"]
              }
            }
          ]
        },
        {
          "module": "@hapi/good-console"
        },
        "stdout"
      ]
    }
  }
}
